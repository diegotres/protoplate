<!doctype html>
<!--[if lt IE 7]>                                          <html manifest="Mmanifest.appcache" class="no-js ie lt-ie10 lt-ie9 lt-ie8 lt-ie7 ie6"> <![endif]-->
<!--[if IE 7]>                                             <html manifest="Mmanifest.appcache" class="no-js ie lt-ie10 lt-ie9 lt-ie8 ie7">        <![endif]-->
<!--[if IE 8]>                                             <html manifest="Mmanifest.appcache" class="no-js ie lt-ie10 lt-ie9 ie8">               <![endif]-->
<!--[if IE 9]>                                             <html manifest="Mmanifest.appcache" class="no-js ie lt-ie10 ie9">                      <![endif]-->
<!--[if IEMobile 7]>                                       <html manifest="Mmanifest.appcache" class="no-js ie iem7">                             <![endif]-->
<!--[if (gt IE 9)|!(IE)|(gt IEMobile 7)|!(IEMobile)]><!--> <html manifest="Mmanifest.appcache" class="no-js">                                 <!--<![endif]-->
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible"                 content="IE=edge,chrome=1">

    <!-- mobile friendly -->
    <meta http-equiv="cleartype"                       content="on">
    <meta name="HandheldFriendly"                      content="True">
    <meta name="MobileOptimized"                       content="320">
    <meta name="viewport"                              content="width=device-width,initial-scale=1">
    <meta name="apple-mobile-web-app-capable"          content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-title"            content="MiniBril">

    <!-- about: site name -->
    <meta property="og:site_name"                      content="">

    <!-- about: image -->
    <meta property="og:image"                          content="">
    <meta itemprop="image"                             content="">

    <!-- about: page url -->
    <meta property="og:url"                            content="">

    <!-- about: page title -->
    <title>MiniBril</title>
    <meta property="og:title"                          content="">
    <meta itemprop="name"                              content="">

    <!-- about: page description -->
    <meta name="description"                           content="">
    <meta property="og:description"                    content="">
    <meta itemprop="description"                       content="">

    <!-- favicons -->
    <%= favicon_tag 'touch/apple-touch-icon-144x144-precomposed.png', :rel => 'apple-touch-icon-precomposed', :sizes => '144x144' %>
    <%= favicon_tag 'touch/apple-touch-icon-114x114-precomposed.png', :rel => 'apple-touch-icon-precomposed', :sizes => '114x114' %>
    <%= favicon_tag 'touch/apple-touch-icon-72x72-precomposed.png',   :rel => 'apple-touch-icon-precomposed', :sizes => '72x72' %>
    <%= favicon_tag 'touch/apple-touch-icon-57x57-precomposed.png',   :rel => 'apple-touch-icon-precomposed', :sizes => '57x57' %>
    <%= favicon_tag 'touch/apple-touch-icon.png',                     :rel => 'shortcut icon' %>

    <!-- stylesheet -->
    <%= stylesheet_link_tag "base" %>
    <%#= yield :stylesheet %>

    <!-- javascript -->
    <%= javascript_include_tag "head" %>
  </head>

  <body class="<%= page_classes %>">
    <%#= yield %>

    <!-- javacript -->
    <%= javascript_include_tag  "all" %>
    <%#= yield :javascript %>
  </body>
</html>